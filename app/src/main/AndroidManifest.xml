<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.green.go.gogreen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.green.go.ui.ActivityFeed"
            android:label="@string/title_activity_feed"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.green.go.ui.ActivityBase" />
        <activity
            android:name="com.green.go.ui.ActivityDenunciar"
            android:label="@string/title_activity_denunciar"
            android:parentActivityName="com.green.go.ui.ActivityFeed"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.green.go.ui.ActivityFeed" />
        </activity>
    </application>

</manifest>